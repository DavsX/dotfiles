alias ..="cd .."
alias cal="cal -m"
alias clr="clear"
alias cp="cp -i"
alias cpr="cp -i -r"
alias df="df -h"
alias dus="du -shc .??* * | sort -h"
alias fprove='forkprove -Mlib::require::all=t/lib,lib'
alias g="git"
alias grep='grep --exclude-dir=.git'
alias gst='git status'
alias ipcalc="ipcalc -n"
alias iptraf="iptraf -u"
alias la="ls -lha"
alias less="less -R"
alias ll="ls -lh"
alias lla="ls -lha"
alias ls="ls --color=auto"
alias mv="mv -i"
alias pl="perl"
alias py="python"
alias pynit="touch __init__.py"
alias reload="source /home/davs/.bashrc"
alias rm="rm -i"
alias sshx="ssh -oStrictHostKeyChecking=no -oUserKnownHostsFile=/tmp/this_is_not_existing_i_hope/known_hosts"
alias strace="strace -s 4096"
alias tree="tree -C"
alias up="cd \`git root\`"
alias upt="cd \`git root\` && cd t"
alias venv="if [ -e venv ]; then source venv/bin/activate; else source /opt/python/bin/activate; fi;"
alias v="vim-term"
alias vi="vim-term"
alias vim="vim-term"
alias vp="touch .vimproject"
alias w0="wget -O /dev/null"
